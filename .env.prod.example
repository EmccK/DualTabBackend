# ========================================
# 生产环境配置模板
# 使用方法: cp .env.prod.example .env.prod
# ========================================

# 数据库配置
POSTGRES_USER=dualtab
POSTGRES_PASSWORD=your-strong-password-here
POSTGRES_DB=dualtab

# JWT 密钥(生成: openssl rand -base64 32)
JWT_SECRET=your-random-jwt-secret-key-here

# 管理员账号
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-admin-password-here

# 端口配置
BACKEND_PORT=8080
ADMIN_PORT=3000

# 上传文件 URL (改为你的服务器 IP 或域名)
UPLOAD_URL=http://your-domain.com:8080/uploads

# ⚠️ 前端 API 地址 (运行时配置,改为你的服务器 IP 或域名)
API_URL=http://your-domain.com:8080
